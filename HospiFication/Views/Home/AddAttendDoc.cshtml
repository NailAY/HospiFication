@model HospiFication.Models.AttendingDoc

@{
	ViewBag.Title = "Добавление нового лечащего врача";
}

    <form asp-action="AddAttendDoc" asp-controller="home" method="post" enctype="multipart/form-data">
        
        <div class="form-group">
            <label asp-for="Attending_Doc_FIO" class="control-label">ФИО лечащего Врача</label>
            <input type="text" asp-for="Attending_Doc_FIO" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="Password" class="control-label">Пароль</label>
            <input type="text" asp-for="Password" class="form-control" />
        </div>

        <div class="form-group">
            <input type="submit" value="Отправить" class="btn btn-default" />
        </div>
        <div class="form-group">
            <a asp-action="AttendDocs" class="btn btn-primary">Назад</a>
        </div>
    </form>
