@model HospiFication.Models.AttendingDoc
@{
	ViewBag.Title = "Удаление Лечащего врача";
}

<div>
    <dl class="dl-horizontal">

        <dt>ID</dt>
        <dd>
            @Html.DisplayFor(model => model.AttendingDocID)
        </dd>

        <dt>ФИО</dt>
        <dd>
            @Html.DisplayFor(model => model.Attending_Doc_FIO)
        </dd>

    </dl>
	<div>
		<form asp-controller="Home" asp-action="DeleteAttend" method="post">
			<div class="form-group">
				<input type="submit" class="btn btn-light" value="Удалить" />
			</div>
		</form>
        <div class="form-group">
            <a asp-action="AttendDocs" class="btn btn-primary">Назад</a>
        </div>
	</div>
</div>
